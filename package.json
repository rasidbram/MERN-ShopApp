{
  "name": "react-shopapp",
  "version": "1.0.0",
  "description": "react shop app",
  "main": "index.js",
  "engine": {
    "node": "v12.18.0",
    "npm": "6.14.8"
  },
  "scripts": {
    "start": "node server/index.js",
    "backend": "nodemon server/index.js",
    "frontend": "npm run front --prefix client",
    "dev": "concurrently \"npm run backend\" \"npm run start --prefix client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "rashid",
  "license": "ISC",
  "dependencies": {
    "async": "^3.2.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongoose": "^5.10.14",
    "multer": "^1.4.2",
    "react-redux": "^5.1.2",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^2.0.6"
  }
}
